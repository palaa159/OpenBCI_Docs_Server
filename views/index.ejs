<!doctype html>
<html>
<head>
  <meta charset='utf-8'>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">

  <title><%= title %></title>
  <link  href='https://cdn.rawgit.com/rstacruz/flatdoc/v0.9.0/theme-white/style.css' rel='stylesheet'>

  <!-- Meta -->
  <meta content="Your Project" property="og:title">
  <meta content="Your Project description goes here." name="description">


</head>
<body role='flatdoc'>

  <div class='header'>
    <div class='left'>
      <h1>OpenBCI</h1>
      <ul>
        <li><a href='https://github.com/openbci/docs'>View on GitHub</a></li>
        <li><a href='https://github.com/openbci/docs/issues'>Issues</a></li>
      </ul>
    </div>
    <div class='right'>
      <!-- GitHub buttons: see http://ghbtns.com -->
      <iframe src="http://ghbtns.com/github-btn.html?user=openbci&amp;repo=docs&amp;type=watch&amp;count=true" allowtransparency="true" frameborder="0" scrolling="0" width="110" height="20"></iframe>
    </div>
  </div>

  <div class='content-root'>
    <div class='menubar'>
      <div class='menu section' role='flatdoc-menu'></div>
    </div>
    <div role='flatdoc-content' class='content'></div>
  </div>

  <!-- Flatdoc -->
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <!-- For old browsers -->
  <script src='https://cdn.rawgit.com/rstacruz/flatdoc/v0.9.0/legacy.js'></script>
  <!-- Relevant things are here -->
  <script src='public/js/flatdoc.js'></script>
  <!-- Flatdoc theme scrolling stuff -->
  <script src='https://cdn.rawgit.com/rstacruz/flatdoc/v0.9.0/theme-white/script.js'></script>
    <!-- Initializer -->
  <script>
    Flatdoc.run({
      fetcher: Flatdoc.file('https://rawgit.com/palaa159/Docs/master/<%= mdUrl %>')
    });
    var prefixUrl = "<%= prefixUrl %>",
        mdList = JSON.parse('<%-JSON.stringify(mdList)%>');
  </script>
  <script src='public/js/main.js'></script>

</body>
</html>